	<!-- This application has been upgraded to support fast-browsing: https://developer.m-files.com/Frameworks/User-Interface-Extensibility-Framework/Fast-Browsing/ -->
	<required-mfiles-version>20.12.0.0</required-mfiles-version>
	<platforms>
		<platform>Desktop</platform>
	</platforms>
	<modules>
		<module environment="shellui" fast-browsing-compatible="true">
			<file>main.js</file>
